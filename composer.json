{
  "name": "jojo1981/json-path-ast-builder",
  "description": "Json Path AST builder",
  "keywords": [
    "json-path",
    "json",
    "path",
    "syntax",
    "semantic",
    "tokenize",
    "query",
    "token",
    "parser",
    "lexer",
    "ast",
    "abstract",
    "tree",
    "traversal",
    "visitor"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "antlr/antlr4-php-runtime": "^0.3.0",
    "antlr4/antlr4": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\JsonPathAstBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\Jojo1981\\JsonPathAstBuilder\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}
